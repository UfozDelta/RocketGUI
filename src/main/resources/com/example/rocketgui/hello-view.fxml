    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button onAction="#onHelloButtonClick" prefHeight="25.0" prefWidth="467.0" text="Hello!" />
    <Button onAction="#onExitButtonClick" prefHeight="25.0" prefWidth="467.0" text="Hell@" />
    <Label fx:id="welcomeText" />
    <Label fx:id="infoText" />
</VBox>
